         <div type="apparatus">
           <listApp>
            <app loc="1"><note>perhaps <foreign xml:lang="grc">
                  <supplied reason="lost">καὶ ὁ δῆ</supplied>
                  <unclear>μ</unclear>ος ἐ<supplied reason="lost">τείμησαν</supplied>
               </foreign>
            </note></app>
            <app loc="2"><note>just possibly e.g. <foreign xml:lang="grc">
                  <unclear>ἀ</unclear>λλα<choice>
                     <corr>ς</corr>
                  </choice> ἀ<unclear>ξ</unclear>
                  <supplied reason="lost">ίας</supplied>
               </foreign>
            </note></app>
            <app loc="4"><note>e.g.  <foreign xml:lang="grc">
                  <supplied reason="lost">χρυ</supplied>σῳ στε<supplied reason="lost">φανῳ</supplied>
               </foreign> or <foreign xml:lang="grc">
                  <supplied reason="lost">εἴ</supplied>σωστε<supplied reason="lost">ι</supplied>
               </foreign>
            </note></app>
           </listApp>
