<ab><lb n="1"/>  <persName type="emperor"><name nymRef="#Ἀγριππῖνα">Ἀγριππῖναν</name>  
   <lb n="2"/><persName type="emperor"><name nymRef="#Μᾶρκος">Μάρκου</name>  <name nymRef="#Ἀγρίππα">Ἀγρίππ<supplied reason="lost">α</supplied></name></persName>
   <lb n="3"/><w lemma="γυνή">γυναῖκα</w>  <w lemma="δέ">δὲ</w>  <persName type="emperor"><addName nymRef="#Γερμανικός">Γε<supplied reason="lost">ρ</supplied>
      <lb n="4" break="no"/>μανικοῦ</addName>  <addName nymRef="#Καῖσαρ">Καίσ<unclear>α</unclear>
         <lb n="5" break="no"/>  ρος</addName></persName>  <w lemma="μήτηρ">μητέρα</w>  <persName type="emperor"><name nymRef="#Γάϊος">Γαΐ
            <lb n="6" break="no"/>ου</name>  <addName nymRef="Αὐτοκράτωρ">Αὐτοκράτο
      <lb n="7" break="no"/>ρος</addName>  <addName nymRef="Σεβαστός">Σεβαστοῦ</addName>      <g ref="#star"/>
            <lb n="8"/>    <space quantity="2" unit="character"/>  <name nymRef="#Καῖσαρ">Καίσαρος</name></persName></persName>  <space quantity="2" unit="character"/>  </ab>
