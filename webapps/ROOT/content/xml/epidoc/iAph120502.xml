   <lb n="5"/><w lemma="ἀρετή">ἀρετῇ</w>  <w lemma="ἔτι">ἔτι</w>  <w lemma="παντάπασι">παντάπασι</w>  <space quantity="1" unit="character"/>  <w lemma="εἰμί">ὄν
      <lb n="6" break="no"/>τα</w>  <w lemma="νέος">νέον</w>  <w lemma="στεφανόω">ἐστεφάνωσεν</w>  <lb n="7"/><w lemma="δέ">δὲ</w>  <w lemma="καί">καὶ</w>  <w lemma="χρυσός">χρυσῷ</w>  <w lemma="στέφανος">στεφάνῳ</w>  </ab>
