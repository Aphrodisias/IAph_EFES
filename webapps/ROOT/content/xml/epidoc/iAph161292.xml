<ab><lb n="1"/>  <persName type="emperor" ref="#hadrian"><addName nymRef="Αὐτοκράτωρ">Αὐτοκράτο
   <lb n="2" break="no"/>ρα</addName>  <addName nymRef="#Καῖσαρ">Καίσαρα</addName>      
   <lb n="3"/> <persName type="emperor" ref="#trajan"><addName nymRef="θεός"><w lemma="θεός">θεοῦ</w></addName> <name nymRef="#Τραιανός">Τραια  
      <lb n="4" break="no"/>νοῦ</name>  <addName nymRef="#Παρθικός">Παρθι  
         <lb n="5" break="no"/>κοῦ</addName> <add place="overstrike"><w lemma="υἱός">υἱὸν</w></add></persName> <persName type="emperor" ref="#nerua"><addName nymRef="θεός"><w lemma="θεός">θεοῦ</w></addName>  
            <lb n="6"/><name nymRef="#Νέρβα">Νέρουα</name></persName> <w lemma="υἱωνός">υἱω 
               <lb n="7" break="no"/>νὸν</w>  <name nymRef="#Τραιανός">Τραια  
                  <lb n="8" break="no"/>νον</name>  <name nymRef="#Ἁδριανός">Ἁδρια  
                     <lb n="9" break="no"/>νὸν</name>  <addName nymRef="Σεβαστός">Σεβασ  
                        <lb n="10" break="no"/>τὸν</addName></persName>  <w lemma="ὁ">ὁ</w>  <w lemma="δῆμος">Δῆμος</w></ab>
