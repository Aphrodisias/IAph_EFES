   <lb n="1"/><persName type="divine" ref="#theoisebastoi"><addName nymRef="θεός"><w lemma="θεός"><supplied reason="lost">θ</supplied>εοῖς</w></addName>  <name nymRef="Σεβαστός">Σεβαστοῖς</name></persName> <w>τῷ</w> <w>δήμῳ</w> <persName type="divine" ref="#herakles"><name nymRef="Ἡρακλῆς">Ἡρακλεῖ</name></persName> <w>τὸ</w> <w>πρόπυλον</w>
