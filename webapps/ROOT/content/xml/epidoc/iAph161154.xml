            <lb n="4"/><name nymRef="#Ζηνᾶς">Ζ<hi rend="ligature">ην</hi>ᾶ</name></persName>  <w>ἀκροβά<hi rend="ligature">τη</hi>ς</w>  <lb n="5"/><orig>σ<unclear>αλ</unclear>αμβα<unclear>ρ</unclear>τεσ</orig><lb n="6"/><w lemma="ὁ"><hi rend="ligature">τὴν</hi></w>  <w><unclear>κ</unclear>αμάραν</w> <w lemma="ἐκ">ἐκ</w> <lb n="7"/><w lemma="ὁ">τῶν</w> <w>ἰδί<unclear>ων</unclear></w>  <w lemma="κατασκευάζω"><unclear>κα</unclear><supplied reason="lost">τε</supplied>σ<lb n="8" break="no"/>κεύασεν</w> <w lemma="ἐπί">ἐπὶ</w> <persName type="attested"><name nymRef="#Ἄτταλος">Ἀτ<lb n="9" break="no"/>  τάλου</name> <w lemma="ὁ">τοῦ</w> <persName type="attested"><name nymRef="#Ἄδραστος"><unclear>Ἀδρ</unclear><supplied reason="lost">ά</supplied><lb n="10" break="no"/><supplied reason="lost">στο</supplied>υ</name></persName></persName>  <gap reason="lost" unit="character" quantity="8"/>  </ab>
