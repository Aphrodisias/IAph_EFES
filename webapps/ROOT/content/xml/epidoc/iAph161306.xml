            <div type="textpart" n="Column ii"><ab>
               <lb n="1"/><persName type="attested"><name nymRef="#Μιθροδάτης">Μιθροδάτου</name>  
   <lb n="2"/><w lemma="ὁ">τοῦ</w> <persName type="attested"><name nymRef="#Μιθροδάτης">Μιθροδά
     <lb n="3" break="no"/>του</name></persName></persName>  </ab></div>
            <div type="textpart" n="Column iii"><ab><lb n="1"/><persName type="attested"><name nymRef="#Με-"><seg part="I">Με</seg></name><gap reason="lost" unit="character" extent="unknown"/>  
