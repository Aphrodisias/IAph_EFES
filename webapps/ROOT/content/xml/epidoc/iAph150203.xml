   <lb n="2"/> <persName type="attested"><name nymRef="#Ἀπελλᾶς">Ἀπέλλα</name></persName></persName> <w>θυγατρ<unclear>ὶ</unclear></w> <w lemma="ἔκγονος">ἐκγόνῃ</w> <w lemma="δέ">δ<supplied reason="lost">ὲ</supplied></w>  
